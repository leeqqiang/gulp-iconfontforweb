{
  "name": "gulp-iconfontforweb",
  "description": "Create icon fonts from several SVG icons",
  "version": "0.1.0",
  "homepage": "https://github.com/leeqqiang/gulp-iconfontforweb",
  "author": {
    "name": "Nicolas Froidure",
    "url": "http://www.insertafter.com/blog.html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/leeqqiang/gulp-iconfontforweb.git"
  },
  "bugs": {
    "url": "https://github.com/leeqqiang/gulp-iconfontforweb/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.githubusercontent.com/leeqqiang/gulp-iconfontforweb/master/LICENSE"
    }
  ],
  "main": "src/index.js",
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "test": "node_modules/mocha/bin/mocha tests/*.mocha.js",
    "coveralls": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --report lcovonly -- tests/*.mocha.js -R spec -t 5000 && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "cover": "./node_modules/istanbul/lib/cli.js cover --report html ./node_modules/mocha/bin/_mocha -- tests/*.mocha.js -R spec -t 5000"
  },
  "keywords": [
    "gulpplugin",
    "gulp",
    "icon",
    "font"
  ],
  "dependencies": {
    "gulp-util": "~2.2.19",
    "gulp-svg2ttf": "0.0.9",
    "gulp-ttf2eot": "0.0.10",
    "gulp-ttf2woff": "0.0.8",
    "gulp-svgicons2svgfontforweb": "0.1.0",
    "plexer": "0.0.3"
  },
  "devDependencies": {
    "gulp": "~3.8.5",
    "mocha": "~1.20.1",
    "event-stream": "~3.1.5",
    "mocha-lcov-reporter": "0.0.1",
    "coveralls": "~2.10.1",
    "istanbul": "~0.2.13"
  },
  "maintainers": [
    {
      "name": "nfroidure",
      "email": "nfroidure@elitwork.com"
    }
  ],
  "directories": {},
  "readme": "based on gulp-iconfont"
}
